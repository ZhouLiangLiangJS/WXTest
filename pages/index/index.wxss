.nav{
  width: 100%;
  height: 80rpx;
  white-space: nowrap;
}
.nav scroll-view{
  height: 80rpx;
}
.nav text{
  display: inline-block;
  font-size: 30rpx;
  margin:0 45rpx;
  line-height: 65rpx;
